{
	"name": "@commercetools/enzyme-extensions",
	"version": "0.0.0",
	"description": "Enzyme extensions trailored at improving dealing with shallowly rendered wrappers",
	"main": "dist/index.js",
	"scripts": {
      "prebuild": "rm -rf dist; mkdir dist",
      "precommit": "lint-staged",
      "prepublish": "npm run build",
		  "format": "npm run format:md && npm run format:js",
      "build": "babel -d dist src",
      "commitmsg": "commitlint -e $GIT_PARAMS",
      "fix:eslint": "eslint --fix --format=node_modules/eslint-formatter-pretty",
      "format:js": "prettier --write 'src/**/*.js'",
      "format:md": "prettier --parser markdown --write '*.md'",
      "lint": "jest --config .jestrc.lint.json",
      "test": "cross-env NODE_ENV=test jest --config .jestrc.test.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/commercetools/enzyme-extensions.git"
   },
   "engines": {
      "node": ">=8",
      "npm": ">=3"
    },
	"keywords": [],
	"author": "Tobias Deekens <tobias.deekens@commercetools.de>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/commercetools/enzyme-extensions/issues"
	},
	"homepage": "https://github.com/commercetools/enzyme-extensions#readme",
	"dependencies": {
		"common-tags": "1.7.2",
		"react-display-name": "0.2.4"
	},
	"devDependencies": {
		"babel-cli": "6.26.0",
		"babel-core": "6.26.3",
		"babel-plugin-transform-class-properties": "6.24.1",
		"babel-plugin-transform-export-extensions": "6.22.0",
		"babel-plugin-transform-object-rest-spread": "6.26.0",
      "@commitlint/cli": "6.2.0",
      "@commitlint/config-conventional": "6.1.3",
      "babel-eslint": "8.2.3",
      "babel-jest": "22.4.3",
      "babel-preset-env": "1.7.0",
      "check-node-version": "3.2.0",
      "cross-env": "5.1.5",
      "eslint-config-prettier": "2.9.0",
      "eslint-config-xo": "0.22.1",
      "eslint-formatter-pretty": "1.3.0",
      "eslint-plugin-jest": "21.15.1",
      "eslint-plugin-prettier": "2.6.0",
      "eslint": "4.19.1",
      "husky": "0.14.3",
      "jest-runner-eslint": "0.5.0",
      "jest": "22.4.3",
      "lint-staged": "7.1.0",
      "prettier": "1.12.1"
	}
}
