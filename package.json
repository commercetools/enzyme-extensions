{
 "name": "@commercetools/enzyme-extensions",
 "version": "4.0.0",
 "description": "Enzyme extensions tailored at improving dealing with shallowly rendered wrappers",
 "main": "src/index.js",
 "scripts": {
  "precommit": "lint-staged",
  "format": "npm run format:md && npm run format:js",
  "commitmsg": "commitlint -e $GIT_PARAMS",
  "fix:eslint": "eslint --fix --format=node_modules/eslint-formatter-pretty",
  "format:js": "prettier --write 'src/**/*.js'",
  "format:md": "prettier --parser markdown --write '*.md'",
  "lint": "jest --config .jestrc.lint.json",
  "test": "cross-env NODE_ENV=test jest --config .jestrc.test.json",
  "test:watch": "cross-env NODE_ENV=test jest --config .jestrc.test.json --watch",
  "test:ci:coverage": "cross-env NODE_ENV=test npm test -- --no-watchman --coverage && codecov"
 },
 "repository": {
  "type": "git",
  "url": "git+https://github.com/commercetools/enzyme-extensions.git"
 },
 "engines": {
  "node": ">=6",
  "npm": ">=3"
 },
 "files": [
  "src"
 ],
 "keywords": [],
 "author": "Tobias Deekens <tobias.deekens@commercetools.de>",
 "contributors": [
  "Dominik Ferber <dominik.ferber+npm@gmail.com>"
 ],
 "license": "MIT",
 "bugs": {
  "url": "https://github.com/commercetools/enzyme-extensions/issues"
 },
 "homepage": "https://github.com/commercetools/enzyme-extensions#readme",
 "dependencies": {
  "common-tags": "1.8.0",
  "lodash.omit": "^4.5.0",
  "react-display-name": "0.2.4"
 },
 "devDependencies": {
  "@commitlint/cli": "7.0.0",
  "@commitlint/config-conventional": "7.0.1",
  "babel-cli": "6.26.0",
  "babel-core": "6.26.3",
  "babel-eslint": "8.2.6",
  "babel-jest": "23.4.0",
  "babel-preset-env": "1.7.0",
  "babel-preset-react": "6.24.1",
  "babel-preset-stage-0": "^6.24.1",
  "codecov": "3.0.4",
  "cross-env": "5.2.0",
  "enzyme": "3.3.0",
  "enzyme-adapter-react-16": "^1.1.1",
  "eslint": "5.1.0",
  "eslint-config-prettier": "2.9.0",
  "eslint-config-xo": "0.23.0",
  "eslint-formatter-pretty": "1.3.0",
  "eslint-plugin-jest": "21.17.0",
  "eslint-plugin-prettier": "2.6.2",
  "husky": "0.14.3",
  "jest": "23.4.1",
  "jest-runner-eslint": "0.6.0",
  "lint-staged": "7.2.0",
  "prettier": "1.13.7",
  "prop-types": "15.6.2",
  "react": "16.4.1",
  "react-dom": "16.4.1"
 },
 "peerDependencies": {
  "enzyme": "3.x.x"
 }
}
